{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
  },
  "extends": "airbnb-base",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
      "comma-dangle": 0,
      "no-underscore-dangle":0,
      "no-return-await": 0
  }
}